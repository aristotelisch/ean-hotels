<div class="row">
  <div id="search-form" class="col-lg-6 col-lg-offset-3 jumbotron">
    <h1>Find your hotel</h1>
    <p>Where would you like to travel?</p>
    <%= form_for(:search, url: hotels_path, method: :get, html: { class: "form", role: "form" }) do |form| %>
      <div class="form-group">
        <%= form.label :destinationString, "Destination", class: ""%>
        <%= form.text_field :destinationString, class: "input-lg form-control" %>
        <%= form.submit "Search", class: "btn btn-primary btn-lg btn-block" %>
      </div>
    <% end %>
  </div>
</div>
